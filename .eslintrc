{
    "plugins": [
        "react",
        "security"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": [
        "airbnb",
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:security/recommended"
    ],
    "rules": {
        "comma-dangle": "off",
        "no-console": "warn",
        "no-debugger": "warn",
        "react/jsx-filename-extension": "off",
        "react/prefer-stateless-function": "warn",
        "react/prop-types": [
            2,
            {
                "skipUndeclared": true
            }
        ]
    }
}